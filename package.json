{
  "name": "personalcredentialmanager",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:dev": "react-native run-android --variant=developmentDebug",
    "android:prod": "react-native run-android --variant=productionDebug",
    "ios:dev": "react-native run-ios --scheme 'PcmAppDevelopment'",
    "ios:prod": "react-native run-ios --scheme 'PcmAppProduction'",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "prepare": "husky install"
  },
  "dependencies": {
    "react": "17.0.2",
    "react-native": "0.66.4",
    "react-native-config": "^1.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^3.0.1",
    "@types/react": "^17.0.39",
    "@types/react-native": "^0.66.15",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "jest": "^26.6.3",
    "lint-staged": "^12.3.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "prettier": "^2.5.1",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.5.5"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --ext .tsx --ext .ts src/ --fix"
    ],
    "./src/**": [
      "prettier --write ."
    ],
    "*.js": "eslint --cache --fix"
  }
}
